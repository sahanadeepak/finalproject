<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>upload_speech</servlet-name>
        <servlet-class>upload_speech</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>receiveAppDet</servlet-name>
        <servlet-class>receiveAppDet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>capture_speech</servlet-name>
        <servlet-class>capture_speech</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>verify_app</servlet-name>
        <servlet-class>verify_app</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>upload_speech</servlet-name>
        <url-pattern>/upload_speech</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>receiveAppDet</servlet-name>
        <url-pattern>/receiveAppDet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>capture_speech</servlet-name>
        <url-pattern>/capture_speech</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>verify_app</servlet-name>
        <url-pattern>/verify_app</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
            index.jsp
            
        </welcome-file>
    </welcome-file-list>
</web-app>
